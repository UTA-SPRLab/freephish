{
  "manifest_version": 2,
  "name": "FreePhish",
  "version": "1.8",
  "description": "FreePhish Web Extension",
  "permissions": [
    "webRequest",
    "<all_urls>",
    "webRequestBlocking",
    "tabs",
    "activeTab"
  ],
  "background": {
    "scripts": ["js/background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "html/popup.html"
  },
  "web_accessible_resources": [
    "icons/warning.svg"
  ],
  "icons": {
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  }
}
