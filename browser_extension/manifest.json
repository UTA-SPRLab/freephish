{
  "manifest_version": 2,
  "name": "FreePhish",
  "version": "1.2",
  "description": "FreePhish phishing blocker",
  "permissions": [
    "webRequest",
    "<all_urls>",
    "webRequestBlocking",
    "tabs",
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "warning.svg"
  ],
  "icons": {
    "64": "icon64.png",
    "128": "icon128.png"
  }
}
